{
  "name": "ui",
  "version": "0.1.67",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/styles.css"
  },
  "license": "MIT",
  "scripts": {
    "dev:ui": "concurrently \"tsup --config tsup.framer.config.ts --watch\" \"pnpm ngrok\"",
    "ngrok": "ngrok http --domain=esm.ngrok.dev 8000",
    "build": "tsup --config tsup.framer.config.ts && tailwindcss -i ./src/styles.css -o ./dist/styles.css --minify && cp -rf assets dist/assets",
    "release": "pnpm run build && mkdir -p release && cp -Rf dist release/$npm_package_name@$npm_package_version && cp -Rf dist release/$npm_package_name@latest",
    "dev": "concurrently \"tsup src/index.ts --format esm,cjs --dts --external react --watch\" \"tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch\"",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "framer-motion": "*"
  },
  "devDependencies": {
    "@chialab/esbuild-plugin-commonjs": "^0.18.0",
    "@shadergradient/react": "workspace:*",
    "@types/node": "^20.19.27",
    "@types/react": "18.2.73",
    "@types/react-dom": "18.2.23",
    "@types/socket.io": "^3.0.2",
    "@uiw/color-convert": "^1.4.3",
    "@uiw/react-color-shade-slider": "^1.4.3",
    "@uiw/react-color-wheel": "^1.4.3",
    "concurrently": "^7.6.0",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "framer": "^2.4.1",
    "globby": "^14.1.0",
    "query-string": "^7.1.3",
    "react": "^18.3.1",
    "react-slider": "^2.0.6",
    "socket.io": "^4.8.1",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.4.19",
    "tsconfig": "workspace:*",
    "tsup": "^8.5.1",
    "typescript": "5.7.3",
    "zustand": "5.0.1"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.88.0"
  }
}